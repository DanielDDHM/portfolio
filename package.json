    "@expo/webpack-config": "^0.17.0",
    "@types/react-native-modal-dropdown": "^0.7.2",
    "dotenv": "^16.0.1",
    "eas-cli": "^1.1.0",
    "expo": "~46.0.7",
    "expo-font": "^10.2.0",
    "expo-screen-orientation": "^4.3.0",
    "expo-status-bar": "~1.4.0",
    "husky": "^8.0.1",
    "nodemailer": "^6.7.8",
    "prettier": "^2.7.1",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-native": "0.69.5",
    "react-native-modal-dropdown": "^1.0.2",
    "react-native-responsive": "^1.0.2",
    "react-native-svg": "12.3.0",
