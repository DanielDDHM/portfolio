{
  "name": "portfolio",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "android": "export set NODE_OPTIONS=--openssl-legacy-provider && expo start --android",
    "ios": "expo start --ios",
    "web": "export set NODE_OPTIONS=--openssl-legacy-provider && expo start --web",
    "start": "set NODE_OPTIONS=--openssl-legacy-provider && expo start",
    "prettier": "prettier --write .",
    "build:web": "export set NODE_OPTIONS=--openssl-legacy-provider && EXPO_DEBUG_WEB=true expo export:web",
    "build:android": "export set NODE_OPTIONS=--openssl-legacy-provider && eas build -p android"
  },
  "dependencies": {
    "@expo/match-media": "^0.3.0",
    "@expo/ngrok": "^2.5.0",
